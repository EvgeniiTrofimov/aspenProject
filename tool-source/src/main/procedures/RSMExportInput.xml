<!-- ********************************************* -->
<!-- INPUT DEFINITION FOR THE "RSM Data" EXPORT    -->
<!-- ********************************************* -->

<tool-input encoding="UTF-8" > 

 <!-- ************** -->
 <!-- LOCALE OPTIONS -->
 <!-- ************** -->

 <input name="olcOID" data-type="string" display-type="picklist" display-name="label.list.translateLocale">
  <picklist field-id="olcName">
   <field id="olcName" />
   <filter field="olcPrimaryInd" operator="notEqual" source="constant" value="1" />
  </picklist>
 </input>

 <!-- ************** -->
 <!-- ENTITY OPTIONS -->
 <!-- ************** -->

 <input name="entity" data-type="string" display-type="select" display-name="report.rsm.entityType" default-value="0">
  <option value="RSM" display-name="report.rsm.entityType.resource"/>
  <option value="FDD" display-name="report.rsm.entityType.dataField"/>
  <option value="RLD" display-name="report.rsm.entityType.dataRelation"/>
  <option value="TBD" display-name="report.rsm.entityType.dataTable"/>
  <option value="TBX" display-name="report.rsm.entityType.extendedTable"/>
  <option value="FDX" display-name="report.rsm.entityType.extendedField"/>
  <option value="FSN" display-name="report.rsm.entityType.fieldSet"/>
  <option value="FTR" display-name="report.rsm.entityType.filter"/>
  <option value="RCD" display-name="report.rsm.entityType.referenceCode"/>
  <option value="RTB" display-name="report.rsm.entityType.referenceTable"/>
  <option value="RPT" display-name="report.rsm.entityType.report"/>
  <option value="VWT" display-name="report.rsm.entityType.template"/>
 </input>

</tool-input>