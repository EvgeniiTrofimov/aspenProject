<?xml version="1.0" encoding="UTF-8"?>
<view-template context="dynamic.maEpimsHelper.3900" name="MA EPIMS Error 3900" locked="false">
  <template>
    <tab name="tab.staff.staff.list.detail.ethnicity">
      <row>
        <block>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text bold="true" font-size="large">&lt;span style="color:#00a3cc;"&gt;Validation Message:&lt;/span&gt;</text>
              <text font-size="large">3900 - Invalid/Missing Race/Ethnicity</text>
            </cell>
          </line>
          <line border="bottom" shaded="true">
            <cell border="none" cell-span="1" line-span="1" shaded="true">
              <text bold="true" underline="true" font-size="medium">Check Race/Ethnicity:</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">1.  Make sure that at least one value is populated here.</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text bold="true" font-size="medium">SR08 Ethnic Code</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <property id="relStfPsnOid.psnHispLatInd" label-display="right" label-short="false" />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <embedded-list detail-context="person.race.popup" id="races" popup-height="125" popup-width="350" relationship="relStfPsnOid.relPsnRacOid" selection-type="multiple">
                <field id="racRaceCode" sort="true" />
              </embedded-list>
            </cell>
          </line>
        </block>
      </row>
    </tab>
  </template>
</view-template>

