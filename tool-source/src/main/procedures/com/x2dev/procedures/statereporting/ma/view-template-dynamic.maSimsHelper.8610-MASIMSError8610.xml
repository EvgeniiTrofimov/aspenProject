<view-template context="dynamic.maSimsHelper.8610" name="MA SIMS Error 8610" locked="false">
  <template>
    <tab name="tab.student.std.list.detail">
      <row>
        <block>
          <line border="bottom" shaded="true">
            <text bold="true" font-size="large">&lt;span style="color:#00a3cc;"&gt;Error Code:&lt;/span&gt;</text>
            <text bold="true" font-size="medium">&amp;nbsp;8610&amp;nbsp;</text>
            <spacer height="15" />
          </line>
          <line border="bottom" shaded="true">
            <text bold="true" font-size="medium">Error Description:</text>
            <text font-size="medium">&amp;nbsp;Having no days in attendance (D0E017) is not likely with reported days in membership (DOE018) &amp;nbsp;</text>
            <spacer height="15" />
          </line>
          <line border="none">
            <text font-size="medium">The number of days of membership (DOE018) is not likely for an enrolled student (DOE012 equals 01 or 40) with no days of attendance (DOE017).&amp;nbsp;</text>
            <spacer height="15" />
          </line>
          <line border="none">
            <text font-size="medium">This error commonly occurs for a student who has been out due to medical issues at the start of the year. In such a situation, an exception can be requested from DESE.&amp;nbsp;</text>
            <spacer height="15" />
          </line>
        </block>
      </row>
      <row>
        <block>
          <line border="bottom" shaded="true">
            <text bold="true" font-size="large">&lt;span style="color:#00a3cc;"&gt;Fields To Review Below:&lt;/span&gt;</text>
          </line>
          <line border="none">
            <property alias="DOE 17" label="DOE 17" label-short="false" read-only="true" />
          </line>
          <line border="none">
            <property alias="DOE Adjusted Attendance" label="DOE Adjusted Attendance" label-short="false" />
          </line>
          <line border="none">
            <property alias="DOE 18" label="DOE 18" label-short="false" read-only="true" />
          </line>
          <line border="none">
            <property alias="DOE Adjusted Membership" label="DOE Adjusted Membership" label-short="false" />
          </line>
        </block>
      </row>
    </tab>
  </template>
</view-template>

