<?xml version="1.0" encoding="UTF-8"?>
<export-format procedure-id="EXPDATA-MA-SCS" name="Student Course Schedule" source-table="tblStudent" delimiter-char="," system-owned="true">
  <heading>SCS,STUDENT_COURSE_DATA,</heading>
  <field position="11" name="CreditsEarned">
    <calculationId>SCS-CreditEarned</calculationId>
    <defaultValue>9999</defaultValue>
    <formatExpression>0.00</formatExpression>
    <formatType>2</formatType>
    <maximumLength>5</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <validationPattern>\d*\.\d{1,2}|\d+</validationPattern>
  </field>
  <field position="12" name="LetterGrade">
    <calc-param>letter</calc-param>
    <calculationId>SCS-Grade</calculationId>
    <formatType>0</formatType>
    <maximumLength>2</maximumLength>
    <minimumLength>2</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
  </field>
  <field position="13" name="NumericGrade">
    <calc-param>number</calc-param>
    <calculationId>SCS-Grade</calculationId>
    <defaultValue>0</defaultValue>
    <formatType>0</formatType>
    <maximumLength>5</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
  </field>
  <field position="1" name="LASID">
    <fieldPath>localId</fieldPath>
    <formatType>0</formatType>
    <keyInd>true</keyInd>
    <maximumLength>32</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
  </field>
  <field position="2" name="SASID">
    <fieldPath>stateId</fieldPath>
    <formatType>0</formatType>
    <keyInd>true</keyInd>
    <maximumLength>10</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <validationPattern>\d*</validationPattern>
  </field>
  <field position="3" name="SchoolId">
    <calc-param>DOE 15</calc-param>
    <calculationId>SCS-SchoolId</calculationId>
    <formatType>0</formatType>
    <maximumLength>8</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
  </field>
  <field position="4" name="CourseCode">
    <calculationId>SCS-CourseCode</calculationId>
    <formatType>0</formatType>
    <keyInd>true</keyInd>
    <maximumLength>20</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
  </field>
  <field position="5" name="SubjectArea">
    <calc-param>WA10-MTC</calc-param>
    <calculationId>SCS-SubjectArea</calculationId>
    <formatType>0</formatType>
    <maximumLength>7</maximumLength>
    <minimumLength>5</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
  </field>
  <field position="6" name="Section">
    <calculationId>SCS-SectionNumber</calculationId>
    <formatType>0</formatType>
    <maximumLength>20</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
  </field>
  <field position="7" name="TermCode">
    <calculationId>SCS-TermCode</calculationId>
    <formatType>0</formatType>
    <maximumLength>2</maximumLength>
    <minimumLength>2</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
  </field>
  <field position="8" name="EnrollmentStatus">
    <calculationId>SCS-EnrollStatus</calculationId>
    <formatType>0</formatType>
    <maximumLength>2</maximumLength>
    <minimumLength>2</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
  </field>
  <field position="9" name="CourseLevel">
    <calculationId>SCS-CourseLevel</calculationId>
    <formatType>0</formatType>
    <maximumLength>2</maximumLength>
    <minimumLength>2</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
  </field>
  <field position="10" name="CreditAvailable">
    <calculationId>SCS-Credit</calculationId>
    <defaultValue>9999</defaultValue>
    <formatExpression>0.00</formatExpression>
    <formatType>2</formatType>
    <maximumLength>5</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <validationPattern>\d*\.\d{1,2}|\d+</validationPattern>
  </field>
</export-format>

