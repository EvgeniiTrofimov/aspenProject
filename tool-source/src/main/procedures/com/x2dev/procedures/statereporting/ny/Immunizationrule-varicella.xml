<!-- Varicella compliance rule - combined rule set -->
<compliance-rule>
	<!--  NY-State: Varicella(Chickenpox): 1 dose. Two (2) doses of varicella vaccine are recommended for all students, 
	but NOT required FOR school entry. Vaccine dose administered up TO 4 days BEFORE the minimum INTERVAL OR 12 months of age.  -->
	<rule-set>
		<dose-total count="1" end-grade="-1" /> 
	</rule-set>
	<rule-set operator="or">
		<dose-total count="1" start-grade="0" end-grade="12" /> 
	</rule-set>
</compliance-rule>