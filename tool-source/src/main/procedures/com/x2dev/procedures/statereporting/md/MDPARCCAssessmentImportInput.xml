<!-- ******************************************************************* -->
<!-- INPUT DEFINITIONS FOR THE MD PARCC ASSESSMENT IMPORT                -->
<!-- ******************************************************************* -->

<tool-input allow-school-select="false" district-support="true">
 
    <!-- ********************* -->
    <!-- ASSESSMENT DEFINITION -->
    <!-- ********************* -->
    <input name="asdOid" data-type="string" display-type="picklist" display-name="Assesment Definitions">
     <picklist field-id="asdName" required="true" >
      <field id="asdName" sort="true" />
      <field id="asdType" />
      </picklist>
    </input>
 
    <!-- ***************** -->
    <!--    SASID FIELD    -->
    <!-- ***************** -->
    <input name="sasidField" data-type="character" display-type="hidden" default-value="SASID" />
 
    <!-- *********************** -->
    <!--    SCHOOL YEAR FIELD    -->
    <!-- *********************** -->
    <input name="schoolYearField" data-type="character" display-type="hidden" default-value="ASSESSMENT_YEAR" />
 
    <!-- ****************** -->
    <!--    PERIOD FIELD    -->
    <!-- ****************** -->
    <input name="periodField" data-type="character" display-type="hidden" default-value="PERIOD" />
 
    <!-- ********************* -->
    <!--    TEST CODE FIELD    -->
    <!-- ********************* -->
    <input name="testCodeField" data-type="character" display-type="hidden" default-value="TEST_CODE" />
 
    <!-- ********************* -->
    <!--    TEST DATE FIELD    -->
    <!-- ********************* -->
    <input name="testDateField" data-type="character" display-type="hidden" default-value="ONLINE_UNIT1_TEST_START_DT" />
 
    <!-- ********************* -->
    <!--    TEST SCHOOL FIELD  -->
    <!-- ********************* -->
    <input name="testSchoolField" data-type="character" display-type="hidden" default-value="SCHOOL" />
 
    <!-- ******************************* -->
    <!--    TEST SCHOOL MATCHING FIELD   -->
    <!-- ******************************* -->
    <input name="testSchoolMatchingField" data-type="character" display-type="hidden" default-value="[DOE SCHOOL CODE]" />
 
    <!-- ********************** -->
    <!--    ALIAS PREFIX        -->
    <!-- ********************** -->
    <input name="aliasPrefix" data-type="character" display-type="hidden" default-value="PARCC_" />

    <!-- ********************* -->
    <!--    FIELD COUNT        -->
    <!-- ********************* -->
    <input name="fieldCount" data-type="integer" display-type="hidden" default-value="174" />

    <!-- ***************** -->
    <!--    DEBUG FIELD    -->
    <!-- ***************** -->
    <input name="debug" data-type="boolean" display-type="hidden" default-value="false" />
 
</tool-input>