<!-- ************************************************** -->
<!-- INPUT DEFINITION FOR THE DELETE OLD CRDC PROCEDURE -->
<!-- ************************************************** -->

<tool-input allow-school-select="false" district-support="true" height="560">
   
  <input name="exportsOids" data-type="picklist" display-type="select" display-name="Old CRDC Exports" required="false">
   <picklist field-id="iedId" multiple="true" width="500" height="470" required="false">
    <field id="iedId" sort="true"/>
    <filter field="iedId" operator="in" source="constant" value="EXP-CRDC-SCH-P1;EXP-CRDC-SCH-P2;EXP-CRDC-LEA-P1;EXP-CRDC-LEA-P2" />
   </picklist>
  </input>
  
  <input name="proceduresOids" data-type="picklist" display-type="select" display-name="Old CRDC Procedures" required="false">
   <picklist field-id="prcId" multiple="true" width="500" height="470" required="false">
    <field id="prcId" sort="true"/>
    <filter field="prcId" operator="in" source="constant" value="EXPDATA-CRDC-SCH-P1;EXPDATA-CRDC-SCH-P2;EXPDATA-CRDC-LEA-P1;EXPDATA-CRDC-LEA-P2;CRDC-P2-AP-SETUP;CRDC-PART2-SETUP" />
   </picklist>
  </input>
  
  <input name="reportsOids" data-type="picklist" display-type="select" display-name="Old CRDC Reports" required="false">
   <picklist field-id="rptReportId" multiple="true" width="500" height="470" required="false">
    <field id="rptReportId" sort="true"/>
    <filter field="rptReportId" operator="in" source="constant" value="SYS-CRDC-001;SYS-CRDC-002;SYS-CRDC-003" />
   </picklist>
  </input>
    
</tool-input>