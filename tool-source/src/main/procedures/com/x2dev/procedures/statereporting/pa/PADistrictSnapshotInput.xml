<!-- INPUT DEFINITIONS FOR THE "PA District Snapshot Export"-->
<!-- ********************************************* -->

<tool-input allow-school-select="false" district-support="false">

 <!-- ************** -->
 <!-- DATA PROCEDURE -->
 <!-- ************** -->

 <input name="procedureId" data-type="string" display-type="hidden" default-value="EXPDATA-PA-ORG"/>
 
 <!-- ********************* -->
 <!-- SCHOOL YEAR CONTEXT   -->
 <!-- ********************* -->

 <input name="reportYearContext" data-type="string" display-type="select" display-name="School Year">
