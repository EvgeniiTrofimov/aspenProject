<?xml version="1.0" encoding="UTF-8"?>
<view-template context="dynamic.maSimsHelper.7950" name="MA SIMS Error 7950" locked="false">
  <template>
    <tab name="tab.student.std.list.detail">
      <row>
        <block>
          <line border="bottom" shaded="true">
            <text bold="true" font-size="large">&lt;span style="color:#00a3cc;"&gt;Error Code:&lt;/span&gt;</text>
            <text bold="true" font-size="medium">&amp;nbsp;7950</text>
            <spacer height="15" />
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer />
            </cell>
          </line>
        </block>
      </row>
      <row>
        <block>
          <line border="bottom" shaded="true">
            <text bold="true" font-size="medium">Error Description:&amp;nbsp;</text>
            <text font-size="medium">Previous year graduates should not be reported this year.&amp;nbsp;</text>
            <spacer height="15" />
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text font-size="medium">If a student was reported as a graduate in EOY collection, s/he should not be reported in the current collection unless s/he is returning for SPED services.</text>
            </cell>
          </line>
        </block>
      </row>
      <row>
        <block>
          <line border="bottom" shaded="true">
            <text bold="true" font-size="medium">Steps to resolve:</text>
            <spacer height="15" />
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text font-size="medium">1. Create a Snapshot of Students getting this error.</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text font-size="medium">2. Mass Update DOE Status to 'Do Not Report'</text>
            </cell>
          </line>
        </block>
      </row>
      <row>
        <block>
          <line border="bottom" shaded="true">
            <cell border="none" cell-span="1" line-span="1" shaded="true">
              <text bold="true" font-size="medium">Note:</text>
            </cell>
          </line>
          <line border="bottom">
            <cell border="none" cell-span="1" line-span="1">
              <text font-size="medium">This can be caused by the Summer Start and Summer End dates not being populated with the proper dates for the updated year (District View &gt; District &gt; Setup &gt; Details &gt; SIF Configuration)</text>
              <spacer width="15" />
            </cell>
          </line>
        </block>
      </row>
      <row>
        <block>
          <line border="bottom" shaded="true">
            <text bold="true" font-size="large">&lt;span style="color:#00a3cc;"&gt;Fields To Review Below:&lt;/span&gt;</text>
          </line>
          <line border="none">
            <property alias="DOE Status" label="DOE Status" label-short="false" />
          </line>
        </block>
      </row>
    </tab>
  </template>
</view-template>

