<?xml version="1.0" encoding="UTF-8"?>
<view-template context="dynamic.maEpimsHelper.6620" name="MA EPIMS Error 6620" locked="false">
  <template>
    <tab name="tab.staff.staff.list.detail.ethnicity">
      <row>
        <block>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text bold="true" font-size="large">&lt;span style="color:#00a3cc;"&gt;Validation Message:&lt;/span&gt;</text>
              <text font-size="large">6620 - Expected Days of Attendance must be greater than or equal to Days in Attendance</text>
            </cell>
          </line>
        </block>
      </row>
      <row>
        <block>
          <line border="bottom" shaded="true">
            <cell border="none" cell-span="1" line-span="1" shaded="true">
              <text bold="true" underline="true" font-size="medium">Check Calendar and Attendance:</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">1.  Verify that if the staff has a calendar assigned, SR36 and SR37 do not have a value greater than 0.</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">2.  If the district doesn't use Staff calendars, be sure SR36 and SR37 are populated accordingly.</text>
            </cell>
          </line>
        </block>
      </row>
      <row>
        <column>
          <property alias="SR09" label="SR09 Status" label-short="false" />
          <property id="stfCalendarID" label-short="false" />
          <property alias="SR36-STF ADJUSTED" label="SR36 Adjusted Days Present" label-short="false" />
          <property alias="SR37-STF ADJUSTED" label="SR37 Adjusted Days Expected" label-short="false" />
        </column>
      </row>
    </tab>
  </template>
</view-template>

