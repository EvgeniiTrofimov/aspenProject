<input name="termlyAttendanceEndDate" data-type="date" display-type="date" display-name="Termly Attendance End Date" /> 
<!-- School Workforce Specific Dates -->
<input name="contractStartDate" data-type="date" display-type="date" display-name="Contract Start Date" />
<input name="contractEndDate" data-type="date" display-type="date" display-name="Contract End Date" /> 
<input name="absenceStartDate" data-type="date" display-type="date" display-name="Absence Start Date" /> 
