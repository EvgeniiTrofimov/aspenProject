<!-- Input Date Parameters -->
<!-- School Census and Pupil Referral Unit Dates -->
<input name="censusDate" data-type="date" display-type="date" display-name="Census Date" />  
<input name="termStartDate" data-type="date" display-type="date" display-name="Term Start Date" />
<input name="termEndDate" data-type="date" display-type="date" display-name="Term End Date" /> 
