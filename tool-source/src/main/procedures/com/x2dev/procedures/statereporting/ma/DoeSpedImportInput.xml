<!-- ******************************************* -->
<!-- INPUT DEFINITIONS FOR THE "DOE SPED" IMPORT -->
<!-- ******************************************* -->

<tool-input allow-school-select="true" district-support="true">

 <!-- *********** -->
 <!-- FIELD COUNT -->
 <!-- *********** -->

 <input name="fieldCount" data-type="integer" display-type="text" display-name="import.doe.sped.fieldCount" required="true" default-value="8" />

 <!-- ************** -->
 <!-- SASID POSITION -->
 <!-- ************** -->

 <input name="sasidIndex" data-type="integer" display-type="text" display-name="import.doe.sped.sasidIndex" required="true" default-value="1" />

 <!-- ******************** -->
 <!-- SPED FIELD POSITIONS -->
 <!-- ******************** -->
 
 <input name="doe32Index" data-type="integer" display-type="text" display-name="import.doe.sped.doe32Index" required="false" default-value="2" />
 <input name="doe34Index" data-type="integer" display-type="text" display-name="import.doe.sped.doe34Index" required="false" default-value="3" />
 <input name="doe36Index" data-type="integer" display-type="text" display-name="import.doe.sped.doe36Index" required="false" default-value="4" />
 <input name="doe38Index" data-type="integer" display-type="text" display-name="import.doe.sped.doe38Index" required="false" default-value="5" />
 <input name="doe39Index" data-type="integer" display-type="text" display-name="import.doe.sped.doe39Index" required="false" default-value="6" />
 <input name="doe40Index" data-type="integer" display-type="text" display-name="import.doe.sped.doe40Index" required="false" default-value="7" />
 <input name="spedLastEvalDateIndex" data-type="integer" display-type="text" display-name="import.doe.sped.spedLastEvalDateIndex" required="false" default-value="8" />

</tool-input>