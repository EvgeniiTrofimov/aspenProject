<?xml version="1.0" encoding="UTF-8"?>
<view-template context="district.dst.list.detail" name="FL District Template Survey">
<template>
    <tab name="tab.organization.details">
      <row>
        <column>
          <property id="orgFscCustID" />
          <property id="orgID" />
          <property id="orgName" />
          <property id="relOrgAdrOid.adrPhone01" />
          <property id="relOrgAdrOid.adrPhone02" />
          <property id="relOrgAdrOid.adrAddress01" />
          <property id="relOrgAdrOid.adrStreetNum" display="false" />
          <property id="relOrgAdrOid.adrStreetLet" display="false" />
          <property id="relOrgAdrOid.adrStreetPreD" display="false" />
          <property id="relOrgAdrOid.adrStreetName" display="false" />
          <property id="relOrgAdrOid.adrStreetPostD" display="false" />
          <property id="relOrgAdrOid.adrStreetType" display="false" />
          <property id="relOrgAdrOid.adrUserValid1" display="false" />
          <property id="relOrgAdrOid.adrUserValid2" display="false" />
          <property id="relOrgAdrOid.adrUserValid3" display="false" />
          <property id="relOrgAdrOid.adrAddress02" />
          <property id="relOrgAdrOid.adrAddress03" />
          <property id="relOrgAdrOid.adrCity" display="false" />
          <property id="relOrgAdrOid.adrState" display="false" />
          <property id="relOrgAdrOid.adrPostalCode" display="false" />
          <property id="orgTimeZone" />
        </column>
      </row>
    </tab>
    <tab name="Survey Dates">
      <row show-border="false">
        <block>
          <line>
            <cell>
              <embedded-list id="survey" extended-dictionary-oid="ddxFlOraSurvey" relationship="relOrgOraOid" embedded-edit="true">
                <field alias="ora-survey-year" />
                <field alias="ora-survey-code" />
                <field alias="ora-survey-start-date" sort="true"/>
                <field alias="ora-survey-end-date" />
                <field alias="ora-survey-snapshot-date" />
              </embedded-list>
            </cell>
          </line>
        </block>
      </row>
    </tab>
  </template>
</view-template>

