<?xml version="1.0" encoding="UTF-8"?>
<export-format procedure-id="EXPDATA-TN-ATT" name="TN 049 Student Attendance Format" source-table="tblStudent" system-owned="true">
  <description>RECORD ID,RECORD VERSION,RECORD TYPE,FILLER1,DISTRICT ID,SCHOOL ID,SCHOOL YEAR,INSTR PROGRAM,STUDENT SSN,STUDENT PIN,LOCAL STUDENT KEY,ATTENDANCE DATE,ATTENDANCE TYPE,STATE ASSGN STD ID,FILLER5</description>
  <field position="10" name="RECORD ID">
    <defaultValue>049</defaultValue>
    <formatType>0</formatType>
    <maximumLength>3</maximumLength>
    <minimumLength>3</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
  <field position="20" name="RECORD VERSION">
    <defaultValue>01</defaultValue>
    <formatType>0</formatType>
    <maximumLength>2</maximumLength>
    <minimumLength>2</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
  <field position="30" name="RECORD TYPE">
    <defaultValue>N</defaultValue>
    <formatType>0</formatType>
    <maximumLength>1</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <validationPattern>^[NEDned]{1}$</validationPattern>
    <depth>0</depth>
  </field>
  <field position="40" name="FILLER1">
    <defaultValue>00</defaultValue>
    <formatType>0</formatType>
    <maximumLength>2</maximumLength>
    <minimumLength>2</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
  <field position="50" name="DISTRICT ID">
    <fieldPath>organization1.id</fieldPath>
    <formatType>0</formatType>
    <keyInd>true</keyInd>
    <maximumLength>3</maximumLength>
    <minimumLength>3</minimumLength>
    <paddingChar>0</paddingChar>
    <paddingDirection>1</paddingDirection>
    <sifPath />
    <validationPattern>^[0-9]{3}$</validationPattern>
    <depth>0</depth>
  </field>
  <field position="60" name="SCHOOL ID">
    <calc-param>ATT_SCHOOL</calc-param>
    <calculationId>ATT_CALC_ATTEND</calculationId>
    <formatType>0</formatType>
    <keyInd>true</keyInd>
    <maximumLength>4</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingChar>0</paddingChar>
    <paddingDirection>1</paddingDirection>
    <sifPath />
    <validationPattern>^[0-9]{1,4}$</validationPattern>
    <depth>0</depth>
  </field>
  <field position="70" name="SCHOOL YEAR">
    <calc-param>ATT_SCHOOLYEAR</calc-param>
    <calculationId>ATT_CALC_OTHER</calculationId>
    <formatExpression>yyyy</formatExpression>
    <formatType>0</formatType>
    <keyInd>true</keyInd>
    <maximumLength>4</maximumLength>
    <minimumLength>4</minimumLength>
    <paddingChar>0</paddingChar>
    <paddingDirection>1</paddingDirection>
    <sifPath />
    <validationPattern>^\d{4}$</validationPattern>
    <depth>0</depth>
  </field>
  <field position="80" name="INSTR PROGRAM">
    <calculationId>TN_INSTRPGM_STD</calculationId>
    <defaultValue>01</defaultValue>
    <formatType>0</formatType>
    <keyInd>true</keyInd>
    <maximumLength>2</maximumLength>
    <minimumLength>2</minimumLength>
    <paddingChar>0</paddingChar>
    <paddingDirection>1</paddingDirection>
    <sifPath />
    <validationPattern>^[0-9]{2}$</validationPattern>
    <depth>0</depth>
  </field>
  <field position="90" name="STUDENT SSN">
    <calculationId>ATT_CALC_SSN</calculationId>
    <formatType>0</formatType>
    <maximumLength>9</maximumLength>
    <minimumLength>0</minimumLength>
    <paddingChar>0</paddingChar>
    <paddingDirection>1</paddingDirection>
    <sifPath />
    <validationId>ATT_VAL_SSN</validationId>
    <depth>0</depth>
  </field>
  <field position="100" name="STUDENT PIN">
    <fieldPath>[DOE PIN]</fieldPath>
    <formatType>0</formatType>
    <maximumLength>9</maximumLength>
    <minimumLength>0</minimumLength>
    <paddingChar>0</paddingChar>
    <paddingDirection>1</paddingDirection>
    <sifPath />
    <validationId>ATT_VAL_PIN</validationId>
    <depth>0</depth>
  </field>
  <field position="110" name="LOCAL STUDENT KEY">
    <fieldPath>localId</fieldPath>
    <formatType>0</formatType>
    <keyInd>true</keyInd>
    <maximumLength>10</maximumLength>
    <minimumLength>0</minimumLength>
    <paddingChar>0</paddingChar>
    <paddingDirection>1</paddingDirection>
    <sifPath />
    <validationId>ATT_VAL</validationId>
    <depth>0</depth>
  </field>
  <field position="120" name="ATTENDANCE DATE">
    <calc-param>ATT_ATTENDDATE</calc-param>
    <calculationId>ATT_CALC_ATTEND</calculationId>
    <formatExpression>YYYYMMDD</formatExpression>
    <formatType>0</formatType>
    <keyInd>true</keyInd>
    <maximumLength>8</maximumLength>
    <minimumLength>8</minimumLength>
    <paddingChar>0</paddingChar>
    <paddingDirection>1</paddingDirection>
    <sifPath />
    <validationPattern>^\d{8}$</validationPattern>
    <depth>0</depth>
  </field>
  <field position="130" name="ATTENDANCE TYPE">
    <calc-param>ATT_ATTENDTYPE</calc-param>
    <calculationId>ATT_CALC_ATTEND</calculationId>
    <formatType>0</formatType>
    <maximumLength>1</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>2</paddingDirection>
    <sifPath />
    <validationPattern>^[APDTUXHINY]{1}$</validationPattern>
    <depth>0</depth>
  </field>
  <field position="140" name="STATE ASSGN STD ID">
    <fieldPath>[DOE EIS STATE ID]</fieldPath>
    <formatType>0</formatType>
    <maximumLength>9</maximumLength>
    <minimumLength>0</minimumLength>
    <paddingChar>0</paddingChar>
    <paddingDirection>1</paddingDirection>
    <sifPath />
    <validationId>VAL_EMPTY_VALUE</validationId>
    <validationPattern>^[1-9][0-9]{1,8}$|^$</validationPattern>
    <depth>0</depth>
  </field>
  <field position="150" name="FILLER5">
    <formatType>0</formatType>
    <maximumLength>11</maximumLength>
    <minimumLength>0</minimumLength>
    <paddingDirection>2</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
</export-format>

