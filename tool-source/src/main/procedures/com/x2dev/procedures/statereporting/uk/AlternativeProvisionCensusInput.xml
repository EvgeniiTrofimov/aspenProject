<?xml version="1.0" encoding="UTF-8"?>

<!-- INPUT DEFINITIONS FOR EARLY YEARS CENSUS EXPORT -->

<tool-input allow-school-select="true" district-support="false" height="500">



 <!-- ********** -->
 <!-- DATE RANGE -->
 <!-- ********** -->
 <input name="censusDate" data-type="date" display-type="date" display-name="Census Date" default-value="today" />

    <!-- Procedure ID -->
    <input name="procedureId" data-type="string" display-type="hidden"
        default-value="EXPDATA-UK-EYC" />

<input name="validationMode" data-type="string" display-type="select" display-name="Validation" default-value="true">
    <option value="off" display-name="Off" />
    <option value="on" display-name="Debug" />
    <option value="summary" display-name="Summary" />
</input>

</tool-input>