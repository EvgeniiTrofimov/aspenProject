<?xml version="1.0" encoding="UTF-8"?>
<view-template context="dynamic.maEpimsHelper.default" name="MA EPIMS Error - Default" locked="false">
  <template>
    <tab name="tab.student.std.list.detail">
      <row>
        <block>
          <line border="bottom" shaded="true">
            <text bold="true" font-size="large">&lt;span style="color:#00a3cc;"&gt;Student Information Management System &lt;/span&gt;</text>
          </line>
        </block>
      </row>
      <row>
        <block>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text bold="true" underline="true" font-size="medium">EPIMS Collection Objects</text>
              <spacer height="10" />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">StaffPersonal</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">EmploymentRecord</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">StaffSectionAssignment</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">StaffEvaluation</text>
            </cell>
          </line>
        </block>
      </row>
      <row>
        <block>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text bold="true" underline="true" font-size="medium">EPIMS Publishing Requirements</text>
              <spacer height="10" />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text bold="true" underline="true" font-size="medium">In order for data to publish for a staff member, the following requirements must be met</text>
              <spacer height="10" />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">Aspen Status = Active OR SR27 (Exit/Termination Date) is greater than or equal to the start of the year date</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">SR09 must not be empty</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">LocalId must not be empty</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">StateId must not be empty</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">DOB must not be empty</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">SchoolOid must not be empty</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">The SchoolOid on one of the Staff Position Records must be the same as the SchoolOid on the Staff Record</text>
            </cell>
          </line>
        </block>
      </row>
    </tab>
  </template>
</view-template>

