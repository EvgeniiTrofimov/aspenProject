<?xml version="1.0" encoding="UTF-8"?>
<view-template context="dynamic.maEpimsHelper.4080" name="MA EPIMS Error 4080" locked="false">
  <template>
    <tab name="Staff&amp;nbsp;Record&amp;nbsp;(SR01&amp;nbsp;-&amp;nbsp;SR38) ">
      <row>
        <block>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text bold="true" font-size="large">&lt;span style="color:#00a3cc;"&gt;Validation Message:&lt;/span&gt;</text>
              <text font-size="large">4080 - Invalid/Missing Degree Subject 1 (SR20)</text>
            </cell>
          </line>
        </block>
      </row>
      <row>
        <block>
          <line border="bottom" shaded="true">
            <cell border="none" cell-span="1" line-span="1" shaded="true">
              <text bold="true" underline="true" font-size="medium">Check Degree Information:</text>
            </cell>
          </line>
          <line border="bottom">
            <cell border="none" cell-span="1" line-span="1">
              <text font-size="medium">1.  SR20 must be one of the valid codes for degree subject listed in the EPIMS Data Handbook</text>
            </cell>
          </line>
          <line border="bottom">
            <cell border="none" cell-span="1" line-span="1">
              <text font-size="medium">2.  It must not be blank or null.</text>
            </cell>
          </line>
          <line border="bottom">
            <cell border="none" cell-span="1" line-span="1">
              <text font-size="medium">3.  Verify that the Degree Subject reference table has the proper state code values populated.</text>
            </cell>
          </line>
        </block>
      </row>
      <row show-border="false">
        <block>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer height="10" />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text bold="true" font-size="medium">SR18-26 Degrees</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer />
            </cell>
          </line>
          <line border="none">
            <text>The three most recent degrees will be included in the export.</text>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <embedded-list detail-context="epimsDetail.degreePopup" id="degrees" popup-height="240" popup-width="525" relationship="relStfSfdOid" selection-type="multiple">
                <field id="sfdType" sort="true" />
                <field id="sfdInstitution" sort="true" />
                <field id="sfdDegree" sort="true" />
                <field id="sfdDate" />
              </embedded-list>
            </cell>
          </line>
        </block>
      </row>
    </tab>
  </template>
</view-template>

