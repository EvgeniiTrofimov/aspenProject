<?xml version="1.0" encoding="UTF-8"?>
<export-format procedure-id="EXPDATA-CRDC-SSC" name="CRDC Student Schedule Export Data" source-table="tblStudent" delimiter-char="," wrapper-char="&quot;" system-owned="true">
  <heading>SectionOid,SchoolName,CourseView,StudentOid,StudentName,TookAPTest,QualifyingAPScore,CourseCategory,APCourseCategory</heading>
  <field position="10" name="SectionOid">
    <calc-param>MST_OID</calc-param>
    <calculationId>RetrieverMstInfo</calculationId>
    <formatType>0</formatType>
    <maximumLength>14</maximumLength>
    <minimumLength>4</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
  <field position="11" name="SchoolName">
    <calc-param>SKL_NAME</calc-param>
    <calculationId>RetrieverMstInfo</calculationId>
    <formatType>0</formatType>
    <maximumLength>50</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
  <field position="12" name="CourseView">
    <calc-param>CRS_NUM</calc-param>
    <calculationId>RetrieverMstInfo</calculationId>
    <formatType>0</formatType>
    <maximumLength>50</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
  <field position="20" name="StudentOid">
    <fieldPath>oid</fieldPath>
    <formatType>0</formatType>
    <maximumLength>14</maximumLength>
    <minimumLength>4</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
  <field position="21" name="StudentName">
    <fieldPath>nameView</fieldPath>
    <formatType>0</formatType>
    <maximumLength>50</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
  <field position="30" name="TookAPTest">
    <calculationId>RIRetrieverAP</calculationId>
    <calc-param>Test</calc-param>
    <defaultValue>N</defaultValue>
    <formatExpression>YN</formatExpression>
    <formatType>3</formatType>
    <maximumLength>1</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <validationPattern>[YN]</validationPattern>
    <depth>0</depth>
  </field>
  <field position="40" name="QualifyingAPScore">
    <calculationId>RIRetrieverAP</calculationId>
    <calc-param>Score</calc-param>
    <defaultValue>N</defaultValue>
    <formatExpression>YN</formatExpression>
    <formatType>3</formatType>
    <maximumLength>1</maximumLength>
    <minimumLength>1</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <validationPattern>[YN]</validationPattern>
    <depth>0</depth>
  </field>
  <field position="50" name="CourseCategory">
    <calc-param>CRS_CATEGORY</calc-param>
    <calculationId>RetrieverCrsInfo</calculationId>
    <formatType>0</formatType>
    <maximumLength>25</maximumLength>
    <minimumLength>0</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
  <field position="60" name="APCourseCategory">
    <calc-param>CRS_CATEGORY_AP</calc-param>
    <calculationId>RetrieverCrsInfo</calculationId>
    <formatType>0</formatType>
    <maximumLength>25</maximumLength>
    <minimumLength>0</minimumLength>
    <paddingDirection>0</paddingDirection>
    <sifPath />
    <depth>0</depth>
  </field>
</export-format>

