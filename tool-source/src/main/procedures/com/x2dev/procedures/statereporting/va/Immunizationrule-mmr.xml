<!-- Kindergarten: 2 does of measles and mumps. 1 dose of rubella
Grade 6: Only one dose of mumps containing vaccine for those who have enrolled in kindergarten
prior to 2010 -->
<compliance-rule>
	<rule-set>
		<rule-set>
			<dose-total count="1" grade-level="0" /> 
		</rule-set>
		<rule-set operator="and">
			<dose-total count="2" grade-level="0"/>
		</rule-set>
	</rule-set>		
	<rule-set operator="or">
		<dose-total count="1" grade-level="6"/>
	</rule-set>
</compliance-rule>