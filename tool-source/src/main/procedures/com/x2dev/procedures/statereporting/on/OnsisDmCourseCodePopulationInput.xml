<?xml version="1.0" encoding="UTF-8"?>

<tool-input prompt-download="false" allow-school-select="false" allow-org-select="false" height="500">
 <input name="institutionType" data-type="string" display-type="select" display-name="report.shared.possibleInstitutionCodes" >
  <picklist field-id="rcdCode" required="true" multiple="true">
   <field id="rcdCode" sort="true" />
    <filter field="relRcdRtbOid.rtbUserName" operator="equals" source="constant" value="Institution Type Codes" />
  </picklist>
 </input>

 <input name="isReview" data-type="boolean" display-type="select" display-name="report.shared.runOptions" default-value="true">
   <option value="true" display-name="report.shared.review" />
   <option value="false" display-name="report.shared.commit" />
 </input>
    
</tool-input>