<?xml version="1.0" encoding="UTF-8"?>
<view-template context="dynamic.maEpimsHelper.6720" name="MA EPIMS Error 6720" locked="false">
  <template>
    <tab name="Work&amp;nbsp;Assignments&amp;nbsp;(WA01&amp;nbsp;-&amp;nbsp;WA12) ">
      <row>
        <block>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text bold="true" font-size="large">&lt;span style="color:#00a3cc;"&gt;Validation Message:&lt;/span&gt;</text>
              <text font-size="large">6720 - Principal Job Classification (WA07) required</text>
            </cell>
          </line>
          <line border="bottom" shaded="true">
            <cell border="none" cell-span="1" line-span="1" shaded="true">
              <text bold="true" font-size="medium">There must be one or more work assignment record for all active periods reported with WA07 = 1305 for each school or charter or collaborative school for active records (SR09=01).</text>
            </cell>
          </line>
        </block>
      </row>
      <row>
        <column>
          <property alias="SR09" label="SR09 Status" label-short="false" />
          <property id="stfIDState" label="SR01 MEPID" label-short="false" />
        </column>
      </row>
      <row>
        <block width="500">
          <line border="none">
            <cell align="center" border="left,top" cell-span="1" line-span="1" shaded="eaeff7">
              <text bold="true" font-size="medium">Teacher Schedule Role</text>
            </cell>
            <cell align="center" border="left,top" cell-span="2" line-span="1" shaded="eaeff7">
              <text bold="true" font-size="medium">Description</text>
            </cell>
            <cell align="center" border="left,top" cell-span="1" line-span="1" shaded="eaeff7">
              <text bold="true" font-size="medium">Staff Position WA07</text>
            </cell>
          </line>
          <line border="none">
            <cell align="center" border="left,top" cell-span="1" line-span="1" shaded="false">
              <text font-size="medium">LeadTeacher</text>
            </cell>
            <cell align="center" border="left,top" cell-span="2" line-span="1" shaded="false">
              <text font-size="medium">Lead teacher with the primary responsibility for student learning in the assigned class section.</text>
            </cell>
            <cell align="center" border="left,top" cell-span="1" line-span="1" shaded="false">
              <text font-size="medium">2305, 2307, or 2325</text>
            </cell>
          </line>
          <line border="none">
            <cell align="center" border="left,top" cell-span="1" line-span="1" shaded="false">
              <text font-size="medium">TeamTeacher</text>
            </cell>
            <cell align="center" border="left,top" cell-span="2" line-span="1" shaded="false">
              <text font-size="medium">Team teacher with shared responsibility for student learning within the assigned class section.</text>
            </cell>
            <cell align="center" border="left,top" cell-span="1" line-span="1" shaded="false">
              <text font-size="medium">2306, 2308, or 2325</text>
            </cell>
          </line>
          <line border="none">
            <cell align="center" border="left,top" cell-span="1" line-span="1" shaded="false">
              <text font-size="medium">ContributingProfessional</text>
            </cell>
            <cell align="center" border="left,top" cell-span="2" line-span="1" shaded="false">
              <text font-size="medium">Contributing professional who has the responsibility to provide additional services that support and increase student learning.</text>
            </cell>
            <cell align="center" border="left,top" cell-span="1" line-span="1" shaded="false">
              <text font-size="medium">2310</text>
            </cell>
          </line>
          <line border="none">
            <cell align="center" border="left,top" cell-span="1" line-span="1" shaded="false">
              <text font-size="medium">Paraprofessional</text>
            </cell>
            <cell align="center" border="left,top" cell-span="2" line-span="1" shaded="false">
              <text font-size="medium">Works alongside and assists professional individuals.</text>
            </cell>
            <cell align="center" border="left,top" cell-span="1" line-span="1" shaded="false">
              <text font-size="medium">4100</text>
            </cell>
          </line>
        </block>
      </row>
      <row show-border="false">
        <block>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer height="10" />
            </cell>
          </line>
          <line border="bottom" shaded="true">
            <cell border="none" cell-span="1" line-span="1" shaded="true">
              <text bold="true" underline="true" font-size="medium">Check Staff Position Records:&amp;nbsp;</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">1.  Verify that the SIF Job Function reference table has federal codes populated correctly.</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">2.  Make sure that there's one and only one staff member with a Staff Position record with a SIF Job Function of 2320 - Executive Administration and a SIF Job Classification (WA07) of 1305 - Principal/headmaster/headmistress/head of school.</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="30" />
              <text font-size="medium">*  You can go to Personell View &gt; Participation Top Tab &gt; Positions Side Tab to find all Staff Position Records</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">3.  If your principal changed mid-year, make sure that the dates on the Staff Position records don't overlap.</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">4.  Make sure that the Staff record is assigned to a school.</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">5.  Staff Position records with no end date will report in the active term.</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer width="15" />
              <text font-size="medium">6.  Staff Position records with an end date will report in the active term based on when the end date is.</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer height="10" />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <text bold="true" font-size="medium">WA06-WA12 Staff Positions</text>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <embedded-list detail-context="epimsSIFDetail.positionPopup" id="positions" popup-height="450" popup-width="550" relationship="relStfSfpOid" selection-type="multiple">
                <field id="relSfpSklOid.sklSchoolName" />
                <field alias="SIF SFP Primary Assignment" />
                <field alias="SIF Job Function" />
                <field alias="WA07-SFP" />
                <field alias="WA08-SFP" />
                <field alias="WA12-SFP" />
                <field id="sfpStartDate" />
                <field id="sfpEndDate" />
                <field alias="Adjusted District ID" />
                <field alias="EPIMS SFP Status" />
              </embedded-list>
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer />
            </cell>
          </line>
          <line border="none">
            <cell border="none" cell-span="1" line-span="1">
              <spacer />
            </cell>
          </line>
        </block>
      </row>
    </tab>
  </template>
</view-template>

