<!-- ************************************************************** -->
<!-- INPUT DEFINITIONS FOR CONVERT IEP TO PROGRAM                   -->
<!-- ************************************************************** -->

<tool-input>

  <!-- ************* -->
  <!-- setPreference -->
  <!-- ************* -->
  <input name="setPreference" data-type="string" display-type="select" display-name="Action" default-value="run">
   <option value="run" display-name="Run activation of IEPs" />
   <option value="show" display-name="Show current delayed activation status" />
   <option value="true" display-name="Enable delayed activation" />
   <option value="false" display-name="Disable delayed activation" />
  </input>
    
</tool-input>