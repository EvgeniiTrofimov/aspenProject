<?xml version="1.0" encoding="UTF-8"?>
<view-template context="iep.form.pwn" name="Default Template Form PWN" locked="false" extendedDataDictionaryId="SPED-PWN">
  <template>
    <tab name="Prior&amp;nbsp;Written&amp;nbsp;Notice">
      <row>
        <block>
          <line border="none">
            <cell border="bottom" cell-span="4" line-span="1" shaded="true">
              <text>First Notice Date.&amp;nbsp;</text>
            </cell>
          </line>
          <line border="none">
            <cell border="bottom" cell-span="4" line-span="1">
              <property alias="csc-iepmtg-notice-date1" label-short="false" />
            </cell>
          </line>
          <line border="bottom" shaded="true">
            <cell border="bottom" cell-span="4" line-span="1" shaded="true">
              <text>Parent Informed of Rights and Responsibilities;</text>
            </cell>
          </line>
          <line border="none">
            <radio alias="csc-meet-parentinfo-pwn" value="Website">Parent(s) informed of rights and will access website for Procedural Safeguards Notice.</radio>
          </line>
          <line border="bottom">
            <radio alias="csc-meet-parentinfo-pwn" value="Provided">Parent(s) informed of rights and were provided copy of Procedural Safeguards Notice.</radio>
          </line>
          <line>
            <text>&amp;nbsp;</text>
          </line>
          <line border="bottom">
            <cell>
              <text>The following action is</text>
            </cell>
            <cell>
              <radio alias="csc-action-result" value="Proposed">Proposed</radio>
            </cell>
            <cell>
              <radio alias="csc-action-result" value="Refused">Refused</radio>
            </cell>
          </line>
          <line border="none">
            <cell border="bottom" cell-span="4" line-span="1" shaded="true">
              <text>Select the Action the School Proposed or Refused.&amp;nbsp;</text>
            </cell>
          </line>
          <line border="none">
            <cell border="bottom" cell-span="4" line-span="1">
              <property alias="csc-describe-eval" label-display="false" label-short="false" />
            </cell>
          </line>
          <line border="none">
            <cell border="bottom" cell-span="4" line-span="1" shaded="true">
              <text>Select each evaluation procedure, assessment, record or report the School used in deciding to propose or refuse the action.&amp;nbsp;</text>
            </cell>
          </line>
          <line border="none">
            <cell border="bottom" cell-span="4" line-span="1">
              <property alias="csc-describe-eval-pwn" label-display="false" label-short="false" />
            </cell>
          </line>
          <line border="none">
            <cell border="bottom" cell-span="4" line-span="1" shaded="true">
              <text>&amp;nbsp;Describe any other choices that the School considered and the reasons why those choices were rejected.&amp;nbsp;</text>
            </cell>
          </line>
          <line border="none">
            <cell border="bottom" cell-span="4" line-span="1">
              <property alias="csc-describe-choice" label-display="false" label-short="false" rows="5" />
            </cell>
          </line>
          <line border="none">
            <cell border="bottom" cell-span="4" line-span="1" shaded="true">
              <text>&amp;nbsp;Describe any other factors relevant to the School's proposal or refusal.&amp;nbsp;</text>
            </cell>
          </line>
          <line border="none">
            <cell border="bottom" cell-span="4" line-span="1">
              <property alias="csc-describe-choice-pwn" label-display="false" label-short="false" rows="5" />
            </cell>
          </line>
        </block>
      </row>
    </tab>
  </template>
</view-template>

