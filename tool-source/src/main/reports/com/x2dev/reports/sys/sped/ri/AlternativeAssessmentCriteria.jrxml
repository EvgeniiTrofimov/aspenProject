                        x="18"
                        y="358"
                        width="18"
                        height="18"
                        key="staticText-5"/>
                    <box></box>
                    <textElement>
                        <font fontName="Times New Roman" pdfFontName="Times-Roman" size="12"/>
                    </textElement>
                <text><![CDATA[2.]]></text>
                </staticText>
                <staticText>
