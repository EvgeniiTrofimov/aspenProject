<?xml version="1.0"?>

<!-- *********************************************************************** -->

<!-- INPUT DEFINITIONS FOR THE "Meeting Invitation" MA REPORT -->

<!-- *********************************************************************** -->
<tool-input>
   <!-- ****************** -->
   <!-- Include Meeting Notes -->
   <!-- ****************** -->
   <input name="includeMtgNotes" data-type="boolean" display-type="checkbox" display-name="Include Mtg Notes" default-value="false" />
   <!-- ****************** -->
   <!-- Print Meeting Notes Only -->
   <!-- ****************** -->
   <input name="printMtgNotesOnly" data-type="boolean" display-type="checkbox" display-name="Print Mtg Notes Only" default-value="false" />
   
   <!-- ****************** -->
   <!-- Print Blank Waiver page -->
   <!-- ****************** -->
   <input name="printBlankWaiver" data-type="boolean" display-type="checkbox" display-name="Print Blank Waiver" default-value="false" />
   
   <input name="formDefinitionOid" data-type="string" display-type="hidden" default-value="fmdMaN3       "/>
</tool-input>