<tool-input allow-school-select="true" district-support="true" height="560">

    <input name="startDate" data-type="date" display-type="date" display-name="Start Date" />

    <input name="endDate" data-type="date" display-type="date" display-name="End Date" />

    <input name="includeDetails" data-type="boolean" display-type="checkbox" display-name="Student Details" default-value="false" />

    <input name="includeExcludeStudents" data-type="boolean" display-type="checkbox" display-name="Include students set to Exclude" default-value="true" />

</tool-input>