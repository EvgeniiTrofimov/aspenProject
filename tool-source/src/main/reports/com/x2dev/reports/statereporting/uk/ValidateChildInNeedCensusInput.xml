<?xml version="1.0" encoding="UTF-8"?>

<!-- INPUT DEFINITIONS FOR VALIDATE CHILD IN NEED CENSUS EXPORT -->

<tool-input district-support="false" height="100">

    <!-- Procedure ID -->
    <input name="procedureId" data-type="string" display-type="hidden" default-value="EXPDATA-UK-CINC" />

    <!-- Date Parameters -->
    <input name="censusDate" data-type="date" display-type="date" display-name="Census Date" />  
    <input name="programStartDate" data-type="date" display-type="date" display-name="Start Date" />  
    <input name="programEndDate" data-type="date" display-type="date" display-name="End Date" />  
    
    <!-- Intermediate XSL file location -->
    <input name="intermediateFile" display-name="Intermediate file xsl" data-type="string" 
        display-type="hidden" default-value="" />
                     
</tool-input>